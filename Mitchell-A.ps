/Ybox YarrowCtr YboxHt 2 div add def % -- start of box   
/TailShorten 0 def        % -- shorten arrow for directions

/Xcenter {  % center stack string in X direction
  dup stringwidth pop  % get x width
  2 div neg            % move over by half
  0 rmoveto
  show
  } bind def

% -- begin arrow code from cookbook (code not claimed by PDM)

/arrowdict 14 dict def
arrowdict begin
  /mtrx matrix def
  end

/arrow {
  % 1= x coord tail center
  % 2= y coord tail center
  % 3= x coord of tip
  % 4= y coord of tip
  % 5= thickness of tail
  % 6= thickness of head
  % 7= length of head
  arrowdict begin
    /headlength exch def
    /halfheadthickness exch 2 div def
    /halfthickness exch 2 div def
    /tipy exch def /tipx exch def
    /taily exch def /tailx exch def

    /dx tipx tailx sub def
    /dy tipy taily sub def
    /arrowlength dx dx mul dy dy mul add sqrt def
    /angle dy dx atan def
    /base arrowlength headlength sub def

    /savematrix mtrx currentmatrix def

    tailx taily translate
    angle rotate

    0 halfthickness neg moveto
    base halfthickness neg lineto
    base halfheadthickness neg lineto
    arrowlength 0 lineto
    base halfheadthickness lineto
    base halfthickness lineto
    0 halfthickness lineto
    closepath
    
    savematrix setmatrix
    end
  } bind def

% -- end arrow code from cookbook (code not claimed by PDM)

/DrawCutLine {
  gsave
  .1 setlinewidth
  [10] 0 setdash
  XCUT 18 sub -14 moveto
  0 794 rlineto
  stroke
  -12 796 18 sub moveto
  XEDGE 36 add 0 rlineto
  stroke
  [] 0 setdash
  grestore
  } bind def

/DoDesc {  % stack: TITLE DESC
  /D exch def
  /y Ydesc def
    XCTR y moveto
    FontXSB setfont
    TITLE Xcenter
  /y y SM sub def
  FontXST setfont
  /n 0 def
  /x D 0 get
     stringwidth pop 2 div 
     neg XCTR add def   % move left
  0 1 9 {  % loop through descriptions
    /y y XS sub def
    x y moveto
    D n get show 
    /n n 1 add def
    } for
  } def

/DoVersion { % stack: VERSION
  /y Ytitle def
    XCTR y moveto
    FontXX setfont
    Xcenter  % VERSION
  } def

/DoTitle {  % stack: PAIRS Title2 Title1
  /y y LG 1.5 mul sub def
    XCTR y moveto
    FontSM setfont
    Xcenter
  /y y MD sub def
    XCTR y moveto
    Xcenter
  /y y BG 1.5 mul sub def
    XCTR y moveto
    FontBGB setfont
    Xcenter
  } def

/DoTableNumber {  % stack: N
  /N exch def
  currentpoint
  currentpoint
  currentpoint
  FontXLB setfont
  N stringwidth pop
  /z exch 70 gt def
  z { .5 1 scale } if
  N Xcenter
  z { 2 1 scale } if
  % now use the current point saved above
  XL add moveto
  FontSMB setfont
  (TABLE) Xcenter
  % add the section
  % now use the current point saved above
  /locy SM 2.5 mul XL add def
  locy add moveto
  FontXLB setfont
  SECTION stringwidth pop
  /z exch 70 gt def
  z { .5 1 scale } if
  SECTION Xcenter
  z { 2 1 scale } if
  % now use the current point saved above
  /locy locy XL add def
  locy add moveto
  FontSMB setfont
  (SECTION) Xcenter
  } def

/NORTH { % stack: NextNorth
  FontBGB setfont
  XCTR SM 1 mul moveto % CHANGED for MITCHELL
  (NORTH) Xcenter
  FontXSB setfont
  XCTR 0 Line1AdjNS sub moveto
  Xcenter  % NextNorth
  XCTR 0 Line2Adj sub moveto
  Line2N Xcenter
  } def

/SOUTH { % stack: NextSouth
  FontBGB setfont
  XCTR YEDGE SM 1 mul sub moveto % CHANGED for MITCHELL
  180 rotate
  (SOUTH) Xcenter
  -180 rotate
  FontXSB setfont
  XCTR YEDGE Line1AdjNS add moveto
  180 rotate
  Xcenter  % NextSouth
  -180 rotate
  XCTR YEDGE Line2Adj add moveto
  180 rotate
  Line2S Xcenter
  -180 rotate
  } def

/EAST { % stack: NextEast
  FontBGB setfont
  Indent1 SM -.6 mul add YCTR moveto  % CHANGED FOR MITCHELL
  270 rotate
  (EAST) Xcenter
  -270 rotate
  FontXSB setfont
  Indent1 Line1AdjEW sub YCTR moveto
  270 rotate
  Xcenter  % NextEast
  -270 rotate
  Indent1 Line2Adj sub YCTR moveto
  270 rotate
  Line2E Xcenter
  -270 rotate
  } def

/WEST { % stack: NextWest
  FontBGB setfont
  XEDGE Indent1 SM -.6 mul add sub YCTR moveto  % CHANGED FOR MITCHELL
  90 rotate
  (WEST) Xcenter
  -90 rotate
  FontXSB setfont
  XEDGE Indent1 sub Line1AdjEW add YCTR moveto
  90 rotate
  Xcenter  % NextWest
  -90 rotate
  XEDGE Indent1 sub Line2Adj add YCTR moveto
  90 rotate
  Line2W Xcenter
  -90 rotate
  } def

/StartBox {  % establishes x and y
  .5 setlinewidth
  /x XCTR XboxWide 2 div sub def
  /y Ybox def
  x y moveto
  x XboxWide add y lineto
  stroke
  XCTR y HG sub 24 sub moveto  %changed...
  (Check boards, opponents and positions every round.) Xcenter  % CHANGED...
  } def

/FinishBox {   % driven by x, y and SECTION
  x y moveto
  x y HG sub lineto
  x XboxWide add y HG sub lineto
  x XboxWide add y lineto
  stroke
  /y y HG sub def
  XCTR y 30 add moveto
  /currfont currentfont def
  FontSec setfont
  SECTION Xcenter
  FontMD setfont
% /y y HG YboxInc 1 sub mul sub def  
% /y y HG 2 mul YboxInc 1 sub mul sub def  
  /y y HG 2 div sub def
% /y y SM 17 mul sub def   % CHANGED for MITCHELL
  XCTR y moveto
  (MOVE BOARDS TO NEXT LOWER TABLE) Xcenter % CHANGED for MITCHELL
  } def

/PairsBoxLine {  % driven by x, y and n
                 % stack: NT BD EW NS RD
  /d XS YboxInc mul def
  x y moveto
  x y d sub lineto
  x XboxWide add y d sub lineto
  x XboxWide add y lineto
  stroke
  /y y XS sub def
  x 10 add y moveto
    show  % RD
  x 68 add y moveto
    Xcenter  % NS
  x 126 add y moveto
    Xcenter  % EW
  x 184 add y moveto
    Xcenter  % BD
  x 242 add y moveto
    Xcenter  % NT
  /y y XS YboxInc 1 sub mul sub def  
  } bind def

/IndBoxLine {  % driven by x, y and n
               % stack: NT BD WW SS EE NN RD
  /d XS YboxInc mul def
  x y moveto
  x y d sub lineto
  x XboxWide add y d sub lineto
  x XboxWide add y lineto
  stroke
  /y y XS sub def
  x 10 add y moveto
    show  % RD
  x 47 add y moveto
    Xcenter  % NN
  x 80 add y moveto
    Xcenter  % EE
  x 113 add y moveto
    Xcenter  % SS
  x 146 add y moveto
    Xcenter  % WW
  x 184 add y moveto
    Xcenter  % BD
  x 242 add y moveto
    Xcenter  % NT
  /y y XS YboxInc 1 sub mul sub def  
  } bind def

